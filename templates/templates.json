{
  "integer_type": "u64",
  "string_type": "String",
  "boolean_type": "bool",
  "array_type": "Vec<{{value}}>",
  "optional_type": "Option<{{value}}>",
  "template_files": [
    {
      "template_path": "rust/struct.txt",
      "target_path": "{{dto_name_snake_case}}.rs",
      "strategy": "FOR_EACH_DTO",
      "template_listings" : [
        {
          "name": "uses",
          "item_template": "use super::{{field_type_snake_case}}::{{field_type}};\n"
        },
        {
          "name": "fields",
          "item_template": "{{field_name}}: {{field_type}},\n"
        },
        {
          "name": "getter",
          "item_template": "pub fn get_{{field_name}}(&self) -> &{{field_type}} { &self.{{field_name}} }\n\n"
        }
      ]
    },
    {
      "template_path": "rust/mod.txt",
      "target_path": "mod.rs",
      "strategy": "ONCE",
      "template_listings" : [
        {
          "name": "modules",
          "item_template": "pub mod {{dto_name_snake_case}}\n"
        }
      ]
    }
  ]
}