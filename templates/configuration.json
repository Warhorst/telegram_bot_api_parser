{
  "integer_type": "u64",
  "string_type": "String",
  "boolean_type": "bool",
  "array_type": "Vec<{{{value}}}>",
  "optional_type": "Option<{{{value}}}>",
  "renames" : [
    {
      "from" : "type",
      "to" : "{{snake_case}}_type"
    }
  ],
  "template_files": [
    {
      "template_path": "templates/rust/struct.txt",
      "target_path": "{{name.snake_case}}.rs",
      "resolve_strategy": "FOR_EACH_DTO"
    },
    {
      "template_path": "templates/rust/mod.txt",
      "target_path": "mod.rs",
      "resolve_strategy": "FOR_ALL_DTOS"
    }
  ]
}